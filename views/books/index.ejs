<h2>Search Books</h2>
<form action="/books" method="GET">

<div> 
   <label>Title</label>
   <input type="text" name="title" value="<% searchOptions.title %>">
   <button type="submit"> Search </button>
</div>

<div> 
   <label>Published After</label>
   <input type="date" name="publishedAfter2" value="<%= searchOptions.publishedAfter2 %>">
</div>

<div> 
   <label>Published Before</label>
   <input type="date" name="publishedBefore1" value="<%= searchOptions.publishedBefore1 %>">
</div>
   <button type=submit>Search</button>
</form> 

<br>
<div>
   <% books1.forEach(book => { // "books1" was "book" earlier, same in routes/books %> 
 <div style="display: inline-block"> 
  <div>  <%= book.title %>  </div>
   <img height="150" width="100" src="<%= book.coverImagePath %>" /> 
 </div>
   <% })%> </div>
